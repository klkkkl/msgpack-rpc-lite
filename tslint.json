{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ],
        "cyclomatic-complexity": [
            true,
            5
        ],
        "interface-name": [
            true,
            "never-prefix"
        ],
        "max-line-length": [
            true,
            125
        ],
        "no-unused-expression": [
            true,
            "allow-fast-null-checks"
        ],
        "quotemark": [
            true,
            "single"
        ],
        "space-before-function-paren": [
            true,
            "anonymous"
        ]
    },
    "rulesDirectory": []
}